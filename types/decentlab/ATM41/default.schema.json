    "vapor_pressure": {
      "title": "Vapor pressure",
      "unit": "kPa",
      "type": "number",
      "description": "Vapor pressure"
    },
    "atmospheric_pressure": {
      "title": "Atmospheric pressure",
      "unit": "kPa",
      "type": "number",
