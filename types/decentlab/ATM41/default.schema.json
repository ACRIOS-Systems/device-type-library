{
  "$id": "https://akenza.io/decentlab/atm41/default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Default",
  "type": "object",
  "properties": {
    "solar_radiation": {
      "title": "Solar radiation",
