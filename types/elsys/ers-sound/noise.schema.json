{
    "$id": "https://elsys.se/ers-sound.noise.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Elsys ERS Sound",
    "type": "object",
    "properties": {
        "soundPeak": {
            "title": "Sound Peak",
            "unit": "dB",
            "description": "Peak value in dBspl",
            "type": "number",
            "minimum": 20,
            "maximum": 120
        },
        "soundAvg": {
            "title": "Sound Average",
            "unit": "dB",
            "description": "Average value in dBspl",
            "type": "number",
            "minimum": 20,
            "maximum": 120
        }
    }
}