{
  "$id": "https://elsys.se/ers-desk.occupancy.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Elsys ERS Desk",
  "type": "object",
  "properties": {
    "motion": {
      "title": "Motion",
      "description": "The number of motion counts.",
      "type": "integer",
      "minimum": 0,
      "maximum": 255
    },
    "occupancy": {
      "title": "Occupancy",
      "description": "Space occupancy. 0 = Unoccupied / 1 = Pending(Entering or leaving) / 2 = Occupied.",
      "type": "integer",
      "minimum": 0,
      "maximum": 2
    }
  }
}