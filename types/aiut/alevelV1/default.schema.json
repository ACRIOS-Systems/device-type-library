{
  "$id": "https://akenza.io/aiut/alevelV1/default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Default",
  "type": "object",
  "properties": {
    "currentLevel": {
      "title": "Current level",
      "unit": "%",
      "type": "number",
      "description": "Current tank level %",
      "minimum": 0,
      "maximum": 100
    },
    "isTanking": {
      "title": "Is tanking",
      "description": "Is true on tanking",
      "type": "boolean"
    },
    "isEmptying": {
      "title": "Is emptying",
      "description": "Is true on empting",
      "type": "boolean"
    },
    "hasMeasurementError": {
      "title": "Measurement error",
      "description": "Is true on measurement error",
      "type": "boolean"
    },
    "hasOutOfRangeError": {
      "title": "Out of range error",
      "description": "Is true if an out of range error is detected",
      "type": "boolean"
    },
    "sequenceNumber": {
      "title": "Sequence number",
      "description": "Sequence number",
      "type": "number"
    },
    "temperature": {
      "title": "Temperature",
      "unit": "°C",
      "type": "number",
      "description": "Temperature in °C",
      "minimum": -3276.5,
      "maximum": 3276.5
    }
  }
}
