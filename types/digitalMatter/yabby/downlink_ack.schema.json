    },
    "fwMaj": {
      "title": "Firmware major",
      "type": "integer",
      "description": "Firmware major",
      "hideFromKpis": true
    },
    "fwMin": {
      "title": "Firmware minor",
      "type": "integer",
      "description": "Firmware minor",
      "hideFromKpis": true
    }
