{
  "$id": "https://akenza.io/oxon/oxobutton/default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Default",
  "type": "object",
  "properties": {
    "buttonId": {
      "title": "Button ID",
      "type": "integer",
      "description": "The ID of the pressed buttons",
      "minimum": 0,
      "maximum": 65535
    },
    "imageID": {
      "title": "Image ID",
      "type": "integer",
      "description": "The ID of the Image used when the button sent the uplink",
      "minimum": 0,
      "maximum": 65535
    },
    "temperature": {
      "type": "number",
      "description": "The temperature in °C.",
      "unit": "°C",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "accX": {
      "description": "Acceleration on the X-Axis is g",
      "type": "number",
      "unit": "g",
      "minimum": -8192,
      "maximum": 8191
    },
    "accY": {
      "description": "Acceleration on the Y-Axis is g",
      "type": "number",
      "unit": "g",
      "minimum": -8192,
      "maximum": 8191
    },
    "accZ": {
      "description": "Acceleration on the Z-Axis is g",
      "type": "number",
      "unit": "g",
      "minimum": -8192,
      "maximum": 8191
    }
  }
}