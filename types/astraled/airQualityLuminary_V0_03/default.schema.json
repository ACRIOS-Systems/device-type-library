{
    "$id": "https://akenza.io/astraled/airQualityLuminary_V0_02/default.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "processingType": "uplink_decoder",
    "topic": "default",
    "title": "Default",
    "properties": {
        "temperature": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/temperature/celsius"
        },
        "humidity": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/humidity/percent"
        },
        "voc": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/voc/ppm"
        },
        "co2": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/co2/ppm"
        },
        "eco2": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/eco2/ppm",
            "description": "Estimated concentration of carbon dioxide calculated from known TVOC",
            "title": "eCO2"
        },
        "pm1_0": {
            "title": "Particles 1.0",
            "unit": "μm",
            "type": "integer",
            "description": "Particles less than 1.0 μm in diameter",
            "minimum": 0,
            "maximum": 10000
        },
        "pm2_5": {
            "title": "Particles 2.5",
            "unit": "μm",
            "type": "integer",
            "description": "Particles less than 2.5 μm in diameter",
            "minimum": 0,
            "maximum": 10000
        },
        "pm4_0": {
            "title": "Particles 4.0",
            "unit": "μm",
            "type": "integer",
            "description": "Particles less than 4.0 μm in diameter",
            "minimum": 0,
            "maximum": 10000
        },
        "pm10": {
            "title": "Particles 10",
            "unit": "μm",
            "type": "integer",
            "description": "Particles less than 10 μm in diameter",
            "minimum": 0,
            "maximum": 10000
        },
        "latitude": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/location/schema.json#/$defs/latitude/degrees"
        },
        "longitude": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/location/schema.json#/$defs/longitude/degrees"
