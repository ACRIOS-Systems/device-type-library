            "title": "Light State",
            "type": "integer",
            "description": "State of the Light",
            "minimum": 0,
            "maximum": 255
        },
        "energy": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/electricity/schema.json#/$defs/energy/kiloWattHours",
            "description": "Energy consumption in kWh",
            "title": "Energy"
        }
