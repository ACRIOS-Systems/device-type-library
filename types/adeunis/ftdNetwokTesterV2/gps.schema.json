{
  "$id": "https://akenza.io/adeunis/ftdv2/gps.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "GPS",
  "type": "object",
  "properties": {
    "latitude": {
      "title": "Latitude",
      "unit": "°",
      "type": "number",
      "description": "Latitude",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "longitude": {
      "title": "Longitude",
      "unit": "°",
      "type": "number",
      "description": "Longitude",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "reception": {
      "title": "Reception",
      "description": "Reception scale",
      "type": "string",
      "enum": ["GOOD", "AVERAGE", "POOR"]
    },
    "sats": {
      "title": "Available satellites",
      "type": "integer",
      "description": "Available satellites"
    }
  }
}
