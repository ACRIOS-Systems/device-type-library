{
  "$id": "https://akenza.io/browan/tbdw100.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "TBDW100",
  "type": "object",
  "properties": {
    "temperature": {
      "title": "Temperature",
      "unit": "C°",
      "type": "number",
      "description": "The temperature in °C.",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "count": {
      "title": "Event Count",
      "type": "integer",
      "description": "Count of events",
      "minimum": 0,
      "maximum": 65535
    },
    "time": {
      "title": "Time",
      "unit": "min",
      "type": "integer",
      "description": "Time between events in minutes",
      "minimum": 0,
      "maximum": 65535
    },
    "status": {
      "title": "Status",
      "description": "Status of the tab. open = true, closed = false",
      "type": "boolean"
    }
  }
}