{
  "$id": "https://akenza.io/enginko/MCF-LW12CO2E/ter_pm.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "TER&PM",
  "type": "object",
  "properties": {
    "date": {
      "title": "Date",
      "type": "string",
      "description": "Date string"
    },
    "temperature": {
      "title": "Temperature",
      "unit": "°C",
      "type": "number",
      "description": "The temperature in °C.",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "humidity": {
      "title": "Humidity",
      "unit": "%",
      "type": "number",
      "description": "The relative humidity in %",
      "minimum": 0,
      "maximum": 100
    },
    "pressure": {
      "title": "Pressure",
      "unit": "hpa",
      "description": "Barometric Pressure in hpa",
      "type": "number"
    },
    "pm1": {
      "title": "Particles 1.0",
      "unit": "μg/m3",
      "type": "integer",
      "description": "Particles less than 1.0 μm in diameter",
      "minimum": 0,
      "maximum": 10000
    },
    "pm25": {
      "title": "Particles 2.5",
      "unit": "μg/m3",
      "type": "integer",
      "description": "Particles less than 2.5 μm in diameter",
      "minimum": 0,
      "maximum": 10000
    },
    "pm10": {
      "title": "Particles 10",
      "unit": "μg/m3",
      "type": "integer",
      "description": "Particles less than 10 μm in diameter",
      "minimum": 0,
      "maximum": 10000
    },
    "batteryLevel": {
      "title": "Battery charge",
      "description": "The battery charge in percent.",
      "type": "integer",
      "minimum": 0,
      "maximum": 100,
      "unit": "%"
    }
  }
}
