{
  "$id": "https://akenza.io/dragino/lse01/default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Default",
  "type": "object",
  "properties": {
    "syncID": {
      "title": "Sync ID",
      "description": "Sync ID",
      "type": "integer",
      "minimum": 0,
      "maximum": 65535
    },
    "syncVersion": {
      "title": "Sync version",
      "description": "Sync version",
      "type": "integer",
      "minimum": 0,
      "maximum": 65535
    },
    "applicationType": {
      "title": "Application type",
      "description": "Application type",
      "type": "integer",
      "minimum": 0,
      "maximum": 65535
    },
    "rfu": {
      "title": "RFU",
      "description": "RFU",
      "type": "integer",
      "minimum": 0,
      "maximum": 65535
    },
    "batteryLevel": {
      "title": "Battery charge",
      "description": "The battery charge in percent.",
      "type": "integer",
      "minimum": 0,
      "maximum": 100,
      "unit": "%"
    },
    "date": {
      "title": "Date",
      "type": "string",
      "description": "Date string"
    },
    "frameId": {
      "title": "Frame ID",
      "description": "Frame ID",
      "type": "integer",
      "minimum": 0,
      "maximum": 65535
    },
    "error": {
      "type": "string",
      "title": "Error Message",
      "description": "Error Message",
      "enum": [
        "CONFIGURATION_ERROR",
        "ERROR_READING_INTERNAL_CONFIGURATION",
        "COMMAND_NOT_IMPLEMENTED",
        "COMMUNICATION_ERROR"
      ]
    },
    "data": {
      "type": "string",
      "title": "Data",
      "description": "HEX Data"
    },
    "length": {
      "type": "integer",
      "title": "Length of the payload",
      "description": "Length of the payload"
    },
    "dataPayload": {
      "type": "string",
      "title": "HEX data payload",
      "description": "HEX data payload"
    },

    "temperature": {
      "title": "Temperature",
      "unit": "°C",
      "type": "number",
      "description": "The temperature in °C.",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "humidity": {
      "title": "Humidity",
      "unit": "%",
      "type": "number",
      "description": "The relative humidity in %",
      "minimum": 0,
      "maximum": 100
    },
    "pressure": {
      "title": "Pressure",
      "unit": "hpa",
      "description": "Barometric Pressure in hpa",
      "type": "number"
    },
    "lux": {
      "title": "Lux",
      "type": "number",
      "unit": "lux",
      "description": "Lux value",
      "minimum": 0,
      "maximum": 255
    },
    "voc": {
      "title": "VOC",
      "unit": "ppb",
      "type": "integer",
      "description": "Total Volatile Organic Compunds in ppb",
      "minimum": 0,
      "maximum": 60000
    },
    "co2": {
      "title": "CO2",
      "unit": "ppm",
      "type": "integer",
      "description": "Carbon Dioxide in ppm",
      "minimum": 0,
      "maximum": 10000
    },
    "pm1": {
      "title": "Particles 1.0",
      "unit": "μg/m3",
      "type": "integer",
      "description": "Particles less than 1.0 μm in diameter",
      "minimum": 0,
      "maximum": 10000
    },
    "pm25": {
      "title": "Particles 2.5",
      "unit": "μg/m3",
      "type": "integer",
      "description": "Particles less than 2.5 μm in diameter",
      "minimum": 0,
      "maximum": 10000
    },
    "pm10": {
      "title": "Particles 10",
      "unit": "μg/m3",
      "type": "integer",
      "description": "Particles less than 10 μm in diameter",
      "minimum": 0,
      "maximum": 10000
    },
    "outsideTemperature": {
      "title": "Outside temperature",
      "unit": "°C",
      "type": "number",
      "description": "The outside temperature in °C.",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "windSpeed": {
      "title": "Wind speed",
      "unit": "m/s",
      "type": "number",
      "description": "Wind speed"
    },
    "tenMinutesAvgWindSpeed": {
      "title": "Ten minutes average wind speed",
      "unit": "m/s",
      "type": "number",
      "description": "Ten minutes average wind speed"
    },
    "windDirection": {
      "title": "Wind direction",
      "unit": "°",
      "type": "number",
      "description": "Wind direction"
    },
    "outsideHumidity": {
      "title": "Outside humidity",
      "unit": "%",
      "type": "number",
      "description": "The relative outside humidity in %",
      "minimum": 0,
      "maximum": 100
    },
    "rainRate": {
      "title": "Rain rate",
      "unit": "mm/h",
      "type": "number",
      "description": "Rain rate in mm/h"
    },
    "uv": {
      "title": "Ultraviolet radiation",
      "unit": "mW/cm2",
      "type": "number",
      "description": "Ultraviolet radiation"
    },
    "solarRadiation": {
      "title": "Solar radiation",
      "unit": "W/m²",
      "type": "number",
      "description": "Solar radiation"
    },
    "dayRain": {
      "title": "Day rain",
      "unit": "mm",
      "type": "number",
      "description": "Day rain"
    },
    "dayET": {
      "title": "Day ET",
      "unit": "mm",
      "type": "number",
      "description": "Day ET"
    },
    "soilMoisture": {
      "title": "Soil moisture",
      "unit": "centibar",
      "type": "number",
      "description": "Soil moisture"
    },
    "leafWetness": {
      "title": "Leaf wetness",
      "type": "number",
      "description": "Leaf wetness"
    },
    "forecastIcons": {
      "title": "Forecast icons",
      "type": "number",
      "description": "Leaf wetness"
    },
    "barTrend": {
      "title": "Bar trend",
      "type": "number",
      "description": "Bar trend"
    }
  }
}
