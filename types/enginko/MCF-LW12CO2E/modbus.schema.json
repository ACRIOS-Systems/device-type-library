{
  "$id": "https://akenza.io/enginko/MCF-LW12CO2E/modbus.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Modbus",
  "type": "object",
  "properties": {
    "frameId": {
      "title": "Frame ID",
      "description": "Frame ID",
      "type": "integer",
      "minimum": 0,
      "maximum": 65535
    },
    "error": {
      "type": "string",
      "title": "Error Message",
      "description": "Error Message",
      "enum": [
        "CONFIGURATION_ERROR",
        "ERROR_READING_INTERNAL_CONFIGURATION",
        "COMMAND_NOT_IMPLEMENTED",
        "COMMUNICATION_ERROR"
      ]
    },
    "data": {
      "type": "string",
      "title": "Data",
      "description": "HEX Data"
    },
    "length": {
      "type": "integer",
      "title": "Length of the payload",
      "description": "Length of the payload"
    },
    "dataPayload": {
      "type": "string",
      "title": "HEX data payload",
      "description": "HEX data payload"
    }
  }
}
