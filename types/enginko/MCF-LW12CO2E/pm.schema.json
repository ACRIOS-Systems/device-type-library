      "maximum": 10000
    },
    "pm25": {
      "title": "Particles 2.5",
      "unit": "μg/m3",
      "type": "integer",
      "description": "Particles less than 2.5 μm in diameter",
      "minimum": 0,
      "maximum": 10000
    },
    "pm10": {
      "title": "Particles 10",
      "unit": "μg/m3",
      "type": "integer",
      "description": "Particles less than 10 μm in diameter",
      "minimum": 0,
      "maximum": 10000
    }
  }
