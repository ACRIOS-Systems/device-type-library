{
  "$id": "https://akenza.io/enginko/MCF-LW12CO2E/climate.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Climate",
  "type": "object",
  "properties": {
    "temperature": {
      "title": "Temperature",
      "unit": "°C",
      "type": "number",
      "description": "The temperature in °C.",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "humidity": {
      "title": "Humidity",
      "unit": "%",
      "type": "number",
      "description": "The relative humidity in %",
      "minimum": 0,
      "maximum": 100
    },
    "pressure": {
      "title": "Pressure",
      "unit": "hpa",
      "description": "Barometric Pressure in hpa",
      "type": "number"
    },
    "lux": {
      "title": "Lux",
      "type": "number",
      "unit": "lux",
      "description": "Lux value"
    },
    "voc": {
      "title": "VOC",
      "unit": "ppb",
      "type": "integer",
      "description": "Total Volatile Organic Compunds in ppb",
      "minimum": 0,
      "maximum": 60000
    },
    "co2": {
      "title": "CO2",
      "unit": "ppm",
      "type": "integer",
      "description": "Carbon Dioxide in ppm",
      "minimum": 0,
      "maximum": 10000
    },
    "batteryLevel": {
      "title": "Battery charge",
      "description": "The battery charge in percent.",
      "type": "integer",
      "minimum": 0,
      "maximum": 100,
      "unit": "%"
    },
    "rfu": {
      "title": "RFU",
      "description": "RFU",
      "type": "integer"
    }
  }
}
