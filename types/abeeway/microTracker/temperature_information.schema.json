{
  "$id": "https://akenza.io/abeeway/compactTracker/temperature_information.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Temperature Information",
  "type": "object",
  "properties": {
    "tempState": {
      "title": "Temperature state",
      "description": "Temperature state",
      "type": "string",
      "enum": [
        "NORMAL_TEMPERATURE",
        "HIGH_TEMPERATURE",
        "LOW_TEMPERATURE",
        "FEATURE_NOT_ACTIVATED"
      ]
    },
    "maxTemperature": {
      "title": "Maximal temperature threshold",
      "unit": "째C",
      "type": "number",
      "description": "Maximal temperature threshold in 째C.",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "minTemperature": {
      "title": "Minimal temperature threshold",
      "unit": "째C",
      "type": "number",
      "description": "Minimal temperature threshold in 째C.",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "highCounter": {
      "title": "Maximal counter threshold",
      "type": "number",
      "description": "Maximal counter threshold"
    },
    "lowCounter": {
      "title": "Minimal counter threshold",
      "type": "number",
      "description": "Minimal counter threshold"
    }
  }
}
