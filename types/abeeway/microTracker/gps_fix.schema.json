{
  "$id": "https://akenza.io/abeeway/compactTracker/gps_fix.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "GPS Fix",
  "type": "object",
  "properties": {
    "age": {
      "title": "Age",
      "unit": "s",
      "type": "integer",
      "description": "Age of the measurement in seconds"
    },
    "latitude": {
      "title": "Latitude",
      "unit": "°",
      "type": "number",
      "description": "Latitude",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "longitude": {
      "title": "Longitude",
      "unit": "°",
      "type": "number",
      "description": "Longitude",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "horizontalAccuracy": {
      "title": "Estimated horizontal position error",
      "unit": "m",
      "type": "number",
      "description": "Estimated horizontal position error in meters"
    }
  }
}
