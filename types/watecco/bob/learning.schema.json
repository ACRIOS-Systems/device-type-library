{
  "$id": "https://akenza.io/watecco/bob-learning.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Watecco BoB",
  "type": "object",
  "properties": {
    "learningPerc": {
      "title": "Learning Percent",
      "unit": "%",
      "description": "State of the learning algorithm",
      "type": "number",
      "minimum": 0,
      "maximum": 100
    },
    "vibration": {
      "title": "Vibration",
      "unit": "mg",
      "description": "Vibration in mg",
      "type": "number",
      "minimum": -32767,
      "maximum": 32767
    },
    "temperature": {
      "title": "Temperature",
      "unit": "°C",
      "type": "number",
      "description": "The temperature in °C.",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "learningFrom": {
      "title": "Learning",
      "description": "Either Learning from Zero % or additional Learning",
      "type": "string"
    }
  }
}