{
  "$id": "https://akenza.io/miromico/insight/battery.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Battery",
  "type": "object",
  "properties": {
    "batteryConsumption": {
      "title": "Battery consumption",
      "unit": "uAh",
      "type": "number",
      "minimum": 0,
      "maximum": 65535,
      "desciption": "Current battery consumption."
    },
    "voltage": {
      "title": "Battery voltage",
      "unit": "V",
      "type": "number",
      "minimum": 0,
      "maximum": 24,
      "desciption": "The battery voltage in Volt."
    },
    "error": {
      "title": "Error",
      "type": "string",
      "desciption": "Wrong length of payload"
    }
  }
}
