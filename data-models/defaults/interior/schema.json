{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/defaults/interior/schema.json",
    "title": "Interior",
    "description": "Interior measurements.",
    "type": "object",
    "$defs": {
        "occupancy": {
            "occupied": {
                "measurementType": "akenza.io/data-models/defaults/interior/occupancy/occupied",
                "title": "Occupied",
                "description": "Whether the space is occupied.",
                "type": "boolean"
            }
        },
        "motion": {
            "count": {
                "measurementType": "akenza.io/data-models/defaults/interior/motion/count",
                "title": "Motion Count",
                "description": "The number of detected motion counts.",
                "type": "integer",
                "minimum": 0
            }
        },
        "people": {
            "in": {
                "measurementType": "akenza.io/data-models/defaults/interior/people/in",
                "title": "People moving inside",
                "description": "People moving inside the area.",
                "type": "integer",
                "minimum": 0
            },
            "out": {
                "measurementType": "akenza.io/data-models/defaults/interior/people/out",
                "title": "People moving outside",
                "description": "People moving out of the area.",
                "type": "integer",
                "minimum": 0
            },
            "count": {
                "measurementType": "akenza.io/data-models/defaults/interior/people/count",
                "title": "People count",
                "description": "People in the area.",
                "type": "integer",
                "minimum": 0
            }
        }
    }
}