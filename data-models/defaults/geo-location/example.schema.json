{
    "$id": "https://akenza.io/geo-location/example.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "processingType": "uplink_decoder",
    "topic": "example",
    "title": "Geo-Location Example",
    "properties": {
        "latitude": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/defaults/geo-location/schema.json/#$defs/gps/latitude",
            "minimum": 45.5,
            "maximum": 48.0,
            "hideFromKpis": false
        },
        "longitude": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/defaults/geo-location/schema.json/#$defs/gps/longitude",
            "minimum": 6.0,
            "maximum": 11.0,
            "hideFromKpis": false
        },
        "required": [
            "latitude",
            "longitude"
        ]
    }
}