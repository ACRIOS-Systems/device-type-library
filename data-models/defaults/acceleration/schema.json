{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/defaults/acceleration/schema.json",
    "title": "Acceleration",
    "description": "Acceleration measurements.",
    "type": "object",
    "$defs": {
        "x": {
            "mg": {
                "measurementType": "akenza.io/data-models/defaults/acceleration/x/mg",
                "title": "Acceleration (X-Axis)",
                "description": "Acceleration on the x-axis.",
                "type": "number",
                "unit": "mG"
            }
        },
        "y": {
            "mg": {
                "measurementType": "akenza.io/data-models/defaults/acceleration/y/mg",
                "title": "Acceleration (Y-Axis)",
                "description": "Acceleration on the y-axis.",
                "type": "number",
                "unit": "mG"
            }
        },
        "z": {
            "mg": {
                "measurementType": "akenza.io/data-models/defaults/acceleration/z/mg",
                "title": "Acceleration (Z-Axis)",
                "description": "Acceleration on the z-axis.",
                "type": "number",
                "unit": "mG"
            }
        }
    }
}