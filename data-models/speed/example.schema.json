{
    "$id": "https://akenza.io/acceleration/example.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "processingType": "uplink_decoder",
    "topic": "example",
    "title": "Acceleration Example",
    "properties": {
        "acc-x": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/acceleration/schema.json/#$defs/x/mg",
            "minimum": -180.0,
            "maximum": 180.0,
            "hideFromKpis": false
        },
        "acc-y": {
            "measurementType": "custom/example/acceleration/x/mg",
            "title": "Acceleration - Y (in mG)",
            "description": "Acceleration on the y-axis in mG.",
            "type": "number",
            "unit": "mG",
            "minimum": -180.0,
            "maximum": 180.0,
            "hideFromKpis": false
        },
        "required": [
            "acc-x",
            "acc-y"
        ]
    }
}